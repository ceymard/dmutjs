{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "downlevelIteration": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noStrictGenericChecks": true,
    "experimentalDecorators": true,
    "outDir": "lib",
    "rootDir": "./src",
    "lib": ["es6"],
    "types": ["node"]
  },
}
